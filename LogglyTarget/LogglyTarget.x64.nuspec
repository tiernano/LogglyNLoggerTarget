<?xml version="1.0"?>
<package >
  <metadata>
    <id>LogglyTarget</id>
    <version>0.1.0.0</version>
    <title>Loggly Target for NLog</title>
    <authors>tiernano</authors>
    <owners>tiernano</owners>
    <licenseUrl>http://opensource.org/licenses/MS-RL</licenseUrl>
    <projectUrl>https://github.com/tiernano/LogglyNLoggerTarget/</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>An NLog target for Loggly</description>
    <releaseNotes>MAJOR UPDATE! .NET 4.7 now targeted. NLog 4.6.3 support. Using Flurl for writing to Loggly. Support for multi writes in one call (buffered)</releaseNotes>
    <copyright>Copyright 2019 LSN</copyright>
    <tags>NLog Logging Loggly</tags>
    <dependencies>
      <dependency id="NLog" version="4.6.3" />
      <dependency id="Flurl" version="2.8.2"/>
      <dependency id="Flurl.Http" version="2.4.2"/>
      <dependency id="Newtonsoft.Json" version="12.0.2"/>
    </dependencies>
  </metadata>
  <files>
    <file src="\bin\$configuration$\$target_framework$\logglytarget.dll" target="lib\$target_framework$"/>
  </files>
  
</package>